<section class="faqs">
    <div class="faqs__details">
        <span class="faqs__id">{{ faqs.id }}</span>
        <h2 class="faqs__title section-title section-title--great">{{ faqs.title }}</h2>
        <a href="{{ faqs.link }}" class="faqs__link button button--secondary button--large">{{ faqs.button }}</a>
    </div>
    <ul class="faqs__list">
        {% for item in faqs.cards %}
            <li class="faqs__list-item">
                {% include 'partials/faqs-card.njk' %}
            </li>
        {% endfor %}
    </ul>
</section>